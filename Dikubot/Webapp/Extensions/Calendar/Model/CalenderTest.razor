@page "/CalenderTest"
@using Dikubot.Extensions.Calender
@using Dikubot.DataLayer.Database.Guild.Models.Calendar
@using Dikubot.DataLayer.Database.Guild.Models.Calendar.Events
@inject UserService _userService;
@attribute [AllowAnonymous]


@foreach (var test in _userService.GetUserGlobalModel().GetAllPermisionsCalendars(CalendarModel.EnumCalendarType.Event).Result)
{
    <MudText Typo="Typo.h4">
         @test.Item1.Name
    </MudText>
    foreach (var VARIABLE in test.Item2)
    {<MudText Typo="Typo.body2">
         @VARIABLE.Name
     </MudText>
        
    }
}
@foreach (var test in _userService.GetUserGlobalModel().GetAllViewCalenders(CalendarModel.EnumCalendarType.Event).Result)
{
    <MudText Typo="Typo.h4">
        @test.Item1.Name
    </MudText>
    foreach (var VARIABLE in test.Item2)
    {<MudText Typo="Typo.body2">
         @VARIABLE.Name;
     </MudText>
        
    }
}

<CalenderInput @bind-Value="_model.CalendarsEnumerable">
</CalenderInput>

@code {
    EventModel _model = new EventModel();
}