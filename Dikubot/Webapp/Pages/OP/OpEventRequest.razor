@page "/op/event/request"

@using Dikubot.Webapp.Extensions.Discovery.Links
@using Dikubot.DataLayer.Database.Global.Event
@using Dikubot.Webapp.Authentication
@attribute [Authorize(Roles = Permissions.GlobalAdmin)]

@inject EventRequestMongoService EventRequestMongoService
@inject EventsMongoService EventsMongoService

@using Dikubot.Webapp.Shared.Request


@attribute [Authorize(Roles = Permissions.GlobalAdmin)]

<OpRequstPageHandler TModel="EventModel" 
                     Link="event" 
                     NameOfTModel="eventet" 
                     RequestMongoService="EventRequestMongoService" 
                     MongoService="EventsMongoService" >
    <TemplateHeader>
        <MudTh>Contact Name</MudTh>
        <MudTh>Event Title</MudTh>
    </TemplateHeader>
    <TemplateRow>
        <MudTd DataLabel="Navn">@context.Contact.Name</MudTd>
        <MudTd DataLabel="Beskrivelse">@context.RequestItem.Subject</MudTd>
    </TemplateRow>
</OpRequstPageHandler>



