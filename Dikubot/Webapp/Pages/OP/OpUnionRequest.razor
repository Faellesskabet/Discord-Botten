@page "/op/union/request"
@using Dikubot.Webapp.Shared.Request
@using Dikubot.Webapp.Authentication
@using Dikubot.DataLayer.Database.Global.Union

@inject UnionRequestMongoService UnionRequestMongoService
@inject UnionMongoService UnionMongoService

@attribute [Authorize(Roles = Permissions.GlobalAdmin)]

<OpRequstPageHandler TModel="UnionModel" 
                     Link="Union" 
                     NameOfTmodel="Unionen" 
                     RequestMongoService="UnionRequestMongoService" 
                     MongoService="UnionMongoService" >
    <TemplateHeader>
        <MudTh>Contact Name</MudTh>
        <MudTh>Union title</MudTh>
    </TemplateHeader>
    <TemplateRow>
        <MudTd DataLabel="Navn">@context.Contact.Name</MudTd>
        <MudTd DataLabel="Beskrivelse">@context.RequestItem.Title</MudTd>
    </TemplateRow>
</OpRequstPageHandler>
